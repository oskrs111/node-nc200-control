{
  "name": "node-nc200-control",
  "version": "0.0.2",
  "description": "electron based nc200 tp-Link wifi camera tool",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=../release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=../release-builds",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64  --prune=true --out=../release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oskrs111/node-nc200-control.git"
  },
  "keywords": [
    "nc200",
    "wifi",
    "camera",
    "tp-link"
  ],
  "author": "Oscar Sanz Llopis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oskrs111/node-nc200-control/issues"
  },
  "homepage": "https://github.com/oskrs111/node-nc200-control#readme",
  "dependencies": {
    "ajax-request": "^1.2.3",
    "cookie-session": "^1.3.1",
    "electron": "^1.6.11",
    "simple-node-logger": "^0.93.28",
    "webcomponentsjs": "^1.0.2"
  }
}
